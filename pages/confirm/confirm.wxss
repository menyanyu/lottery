.title{
    font-weight: 600;
    font-size: 30rpx;
    color: #383838;
    padding: 30rpx 0;
    display: flex;
    justify-content: center;
}
.title text{
    width: 560rpx;
    display: flex;
    justify-content: flex-start;
}

.table{
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    
}
/* .table_box{

} */
.tableHeader{
    display: flex;
    justify-content: center;
    align-items: center;
}

.redBallTitle{
    display: flex;
    justify-content: center;
    align-items: center;
    width: 80rpx;
    height: 70rpx;
    color: #ffffff;
    font-size: 24rpx;
    outline: 1rpx solid #aaaaaa;
    background-color: #fa8080;
}
.blueBallTitle{
    display: flex;
    justify-content: center;
    align-items: center;
    width: 80rpx;
    height: 70rpx;
    color: #ffffff;
    font-size: 24rpx;
    outline: 1rpx solid #aaaaaa;
    background-color: #4293FC;
}
.tableContent{
    width: 560rpx;
    height: 70rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    
}
.balls{
    display: flex;
    justify-content: center;
    align-items: center;
    width: 80rpx;
    height: 100%;
    outline: 1rpx solid #aaaaaa;
    background-color: #ffffff;
}

.buttonGroup{
    padding: 50rpx;
    display: flex;
    justify-content: space-around;
    align-items: center;
}


.retry{
    background-color: #4293FC;
    border-radius: 50rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    color: #ffffff;
    height: 80rpx;
    box-shadow: 1px 0 5px #aaaaaa;
    padding: 0 40rpx 5rpx 40rpx;
}
.submit{
    background-color: #4293FC;
    border-radius: 50rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    color: #ffffff;
    height: 80rpx;
    box-shadow: 1px 0 5px #aaaaaa;
    padding: 0 70rpx 5rpx 70rpx;
}
.analysisResultsBox{
    display: flex;
    flex-direction:column;
    align-items: center;
    padding: 30rpx 0 100rpx 0;
}
.openLottery_date,.openLottery_number {
    display: flex;
    justify-content: center;
    width: 560rpx;
    color: #dab200;
    font-weight: 600;
}
