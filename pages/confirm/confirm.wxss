.resultBox{
    padding: 60rpx 0 100rpx 0;
}

.analysisResultContent{
    display: flex;
    justify-content: center;
    flex-wrap: wrap;

}
.numbers{
    width: 80rpx;
    height: 70rpx;
    outline: 1rpx solid #8d8d8d;
    background-color: #ffffff;
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
}
.redNumber{
    color: #ff7474;
}
.blueNumber{
    color: #4495ff;
}

.redTick{
    position: absolute;
    width: 35rpx;
    height: 35rpx;
    bottom: 0;
    right: 5rpx;
}
.blueTick{
    position: absolute;
    width: 35rpx;
    height: 35rpx;
    bottom: 0;
    right: 5rpx;
}
.resultText{
    width: 560rpx;
    display: flex;
    justify-content: center;
    color: #D94D7F;
    font-weight: 600;
    font-size: 30rpx;
    margin: 20rpx 0 50rpx 0;
}